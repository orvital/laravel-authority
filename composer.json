{
    "name": "orvital/laravel-authority",
    "description": "Custom Authentication for Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jona Goldman",
            "email": "jonagoldman@gmail.com"
        }
    ],
    "require": {
        "laravel/framework": "^9.3"
    },
    "require-dev": {
        "laravel/pint": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "Orvital\\Authority\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Orvital\\Authority\\AuthorityServiceProvider",
                "Orvital\\Authority\\Password\\PasswordResetServiceProvider"
            ]
        }
    }
}